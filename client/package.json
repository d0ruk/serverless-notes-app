{
  "name": "notes-app-client",
  "version": "0.2.1",
  "scripts": {
    "start": "node scripts/start.js",
    "build": "node scripts/build.js",
    "test": "node scripts/test.js --env=jsdom"
  },
  "author": "Doruk Kutlu",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/d0ruk/serverless-notes-app.git"
  },
  "license": "MIT",
  "dependencies": {
    "@doruk/components": "^1.7.0",
    "antd": "^3.5.4",
    "aws-amplify": "^0.4.1",
    "lodash": "^4.17.5",
    "object-assign": "4.1.1",
    "promise": "8.0.1",
    "prop-types": "^15.6.0",
    "query-string": "^5.1.0",
    "raf": "3.4.0",
    "random-sentence": "^1.0.4",
    "react": "^16.4.0",
    "react-dom": "^16.4.0",
    "react-file-reader-input": "^1.1.4",
    "react-helmet": "^5.2.0",
    "react-redux": "^5.0.6",
    "react-router-dom": "^4.2.2",
    "react-router-prop-types": "^1.0.3",
    "redux": "^3.7.2",
    "redux-promise-middleware": "^5.0.0",
    "sanitize.css": "^5.0.0",
    "whatwg-fetch": "2.0.3"
  },
  "babel": {
    "presets": [
      "react-app"
    ],
    "plugins": [
      "transform-decorators-legacy",
      [
        "import",
        {
          "libraryName": "antd",
          "style": true
        }
      ]
    ]
  },
  "eslintConfig": {
    "parser": "babel-eslint",
    "parserOptions": {
      "ecmaVersion": 6,
      "sourceType": "module",
      "ecmaFeatures": {
        "jsx": true,
        "generators": true,
        "experimentalDecorators": true,
        "experimentalObjectRestSpread": true
      }
    },
    "extends": "airbnb",
    "env": {
      "browser": true
    },
    "plugins": [
      "import",
      "jsx-a11y",
      "react"
    ],
    "rules": {
      "quotes": 0,
      "comma-dangle": 0,
      "indent": 0,
      "arrow-parens": 0,
      "object-curly-newline": 0,
      "no-debugger": 1,
      "react/sort-comp": 0,
      "react/no-typos": 0,
      "react/require-default-props": 0,
      "react/jsx-indent": 0,
      "react/forbid-prop-types": 1,
      "react/no-unused-prop-types": 1,
      "jsx-a11y/anchor-is-valid": 0,
      "jsx-a11y/heading-has-content": 0,
      "no-use-before-define": [
        2,
        "nofunc"
      ],
      "no-unused-vars": 1
    }
  },
  "devDependencies": {
    "autoprefixer": "^7.2.5",
    "babel-core": "6.26.0",
    "babel-eslint": "^8.2.1",
    "babel-jest": "^22.0.6",
    "babel-loader": "7.1.2",
    "babel-plugin-import": "^1.6.3",
    "babel-plugin-transform-decorators-legacy": "^1.3.4",
    "babel-preset-react-app": "^3.1.0",
    "babel-runtime": "6.26.0",
    "case-sensitive-paths-webpack-plugin": "2.1.1",
    "css-loader": "0.28.7",
    "dotenv": "4.0.0",
    "eslint": "^4.17.0",
    "eslint-config-airbnb": "^16.1.0",
    "eslint-loader": "1.9.0",
    "eslint-plugin-import": "2.8.0",
    "eslint-plugin-jsx-a11y": "^6.0.3",
    "eslint-plugin-react": "^7.6.1",
    "extract-text-webpack-plugin": "3.0.2",
    "file-loader": "^1.1.6",
    "fs-extra": "^5.0.0",
    "html-webpack-plugin": "^2.30.1",
    "less": "^2.7.3",
    "less-loader": "^4.0.5",
    "postcss-flexbugs-fixes": "3.2.0",
    "postcss-import-url": "^3.0.4",
    "postcss-loader": "^2.1.0",
    "postcss-nested": "^3.0.0",
    "react-dev-utils": "^4.2.1",
    "style-loader": "^0.19.1",
    "sw-precache-webpack-plugin": "0.11.4",
    "url-loader": "0.6.2",
    "webpack": "^3.10.0",
    "webpack-bundle-analyzer": "^2.10.0",
    "webpack-dev-server": "^2.11.1",
    "webpack-manifest-plugin": "1.3.2",
    "webpack-notifier": "^1.5.1"
  }
}
